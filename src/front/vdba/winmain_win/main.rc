      BEGIN
        MENUITEM "Unloadd&b...",            IDM_UNLOADDB, GRAYED
        MENUITEM "&Copydb...",              IDM_COPYDB, GRAYED
      END
    MENUITEM SEPARATOR
    MENUITEM "S&QL Scratchpad...\tCtrl+T",        IDM_SQLACT
    MENUITEM SEPARATOR
    MENUITEM "Space &Calculations...",      IDM_SPACECALC
  END

  POPUP "&Operations"
  BEGIN
    MENUITEM "G&enerate Statistics (Optimize Database)...",       IDM_GENSTAT, GRAYED
    MENUITEM "Disp&lay Statistics...",        IDM_DISPSTAT, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Al&ter DB...",                  IDM_ALTERDB, GRAYED
    MENUITEM "A&udit...",                     IDM_AUDIT, GRAYED
    MENUITEM "Mod&ify Structure...\tCtrl+M",  IDM_MODIFYSTRUCT, GRAYED
    MENUITEM "C&reate Index...\tCtrl+I",      IDM_CREATEIDX, GRAYED
    MENUITEM "&Sysmod...\tCtrl+Y",            IDM_SYSMOD, GRAYED
    MENUITEM "User&mod...",                   IDM_USERMOD, GRAYED
    MENUITEM "Verif&y Database...",           IDM_VERIFYDB, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Change &Journaling State...",   IDM_JOURNALING, GRAYED
    MENUITEM "E&xpiration Date...",           IDM_EXPIREDATE, GRAYED
    MENUITEM "Comme&nts...",                  IDM_COMMENT, GRAYED
    MENUITEM "&Fastload...",                  IDM_FASTLOAD, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Enable/Disable Audit Events...", IDM_SECURITYAUDIT, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Install Replication Objects...",IDM_REPLIC_INSTALL,  GRAYED
    MENUITEM "Propagate...",                  IDM_REPLIC_PROPAG,   GRAYED
    MENUITEM "Build Server(s)...",            IDM_REPLIC_BUILDSRV, GRAYED
    MENUITEM "Create Keys...",                IDM_REPLIC_CREATEKEYS, GRAYED
    MENUITEM "Activate Replication...",       IDM_REPLIC_ACTIVATE, GRAYED
    MENUITEM "Deactivate Replication...",     IDM_REPLIC_DEACTIVATE, GRAYED
    MENUITEM "Reconciler...",                 IDM_REPLIC_RECONCIL, GRAYED
    MENUITEM "Arcclean...",                   IDM_REPLIC_ARCCLEAN, GRAYED
    MENUITEM "Repmod...",                     IDM_REPLIC_REPMOD, GRAYED
    MENUITEM "Dereplicate...",                IDM_REPLIC_DEREPLIC, GRAYED
    MENUITEM "Mobile Parameters...",          IDM_REPLIC_MOBILE,   GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Force Refres&h...\tF5",           IDM_REFRESH, GRAYED
  END

  POPUP "&Query"
  BEGIN
    MENUITEM "&Clear",                      IDM_QUERYCLEAR, GRAYED
    MENUITEM "&Open Query...",              IDM_QUERYOPEN, GRAYED
    MENUITEM "&Save Query As...",           IDM_QUERYSAVEAS, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "SQL &Assistant...",           IDM_SQLWIZARD, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "&Go\tF9",                     IDM_QUERYGO, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "S&how Query Result",          IDM_QUERYSHOWRES, GRAYED
  END

  POPUP "&Window"
  BEGIN
    MENUITEM "&Cascade",                    IDM_WINDOWCASCADE
    MENUITEM "&Tile Horizontal",            IDM_WINDOWTILE_HORZ
    MENUITEM "Tile &Vertical",              IDM_WINDOWTILE_VERT
    MENUITEM "&Arrange Icons",              IDM_WINDOWICONS
    MENUITEM SEPARATOR
    MENUITEM "&New Window",                 IDM_NEWWINDOW, GRAYED
    MENUITEM "Tear &Out",                   IDM_TEAROUT, GRAYED
    MENUITEM "&Restart From Position",      IDM_RESTARTFROMPOS, GRAYED
    MENUITEM "&Scratchpad",                 IDM_SCRATCHPAD
    MENUITEM SEPARATOR
    MENUITEM "S&elect Window...",           IDM_SELECTWINDOW
    MENUITEM "Close All &Windows",          IDM_WINDOWCLOSEALL
  END

  POPUP "&Help"
  BEGIN
    MENUITEM "&Contents...",                  IDM_HELPINDEX
    MENUITEM "&Search...",                    IDM_HELPSEARCH
    MENUITEM "Context &Help\tF1",             IDM_CONTEXTHELP
    MENUITEM SEPARATOR
    MENUITEM "&About Visual DBA...", IDM_ABOUT
  END

