    MENUITEM "Colla&pse All Branches\tCtrl+/",  IDM_CLASSB_COLLAPSEALL   , GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Force Refres&h...\tF5",           IDM_REFRESH, GRAYED
    MENUITEM "Activate Bac&kground Refresh",    IDM_CHMODREFRESH
    MENUITEM SEPARATOR
    MENUITEM "&Filters...\tCtrl+R",             IDM_FILTER, GRAYED
    MENUITEM "&Show System Objects",            IDM_SHOW_SYSTEM, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Proper&ties...",                  IDM_PROPERTIES, GRAYED
//    MENUITEM "&Map",                            IDM_MAP, GRAYED
  END

  POPUP "&Database"
  BEGIN
    MENUITEM "Du&plicate DB",               IDM_DUPLICATEDB, GRAYED
    MENUITEM "&Infodb",                     IDM_INFODB, GRAYED
    MENUITEM "Backup (C&heckpoint)...",     IDM_CHECKPOINT, GRAYED
    MENUITEM "Restore (Rollfor&ward DB)...",          IDM_ROLLFORWARD, GRAYED
