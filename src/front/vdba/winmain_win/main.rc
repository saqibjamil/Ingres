// Menu when current doc is on OpenIngres node (no gateway)
ID_MENU_GWNONE MENU DISCARDABLE
BEGIN
  POPUP "&File"
  BEGIN
    MENUITEM "&New\tCtrl+N",                IDM_NEW
    MENUITEM "&Open...\tCtrl+O",            IDM_OPEN
    MENUITEM "&Save\tCtrl+S",               IDM_SAVE
    MENUITEM "Save &As...",                 IDM_SAVEAS
    MENUITEM SEPARATOR
    MENUITEM "&Connect...",                 IDM_CONNECT
    MENUITEM "&Disconnect...",              IDM_DISCONNECT
    MENUITEM SEPARATOR
    MENUITEM "&Print...\tCtrl+P",           IDM_PRINT, GRAYED
    MENUITEM "P&references...",             IDM_PREFERENCES
    MENUITEM SEPARATOR
    MENUITEM "E&xit\tAlt+F4",               IDM_FILEEXIT
  END

  POPUP "&Edit"
  BEGIN
    MENUITEM "Cu&t\tCtrl+X",                IDM_CUT, GRAYED
    MENUITEM "&Copy\tCtrl+C",               IDM_COPY, GRAYED
    MENUITEM "&Paste\tCtrl+V",              IDM_PASTE, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "C&reate...",                  IDM_ADDOBJECT, GRAYED
    MENUITEM "&Alter...",                   IDM_ALTEROBJECT, GRAYED
    MENUITEM "&Drop...",                    IDM_DROPOBJECT, GRAYED
    MENUITEM "&Grant...",                   IDM_GRANT, GRAYED
    MENUITEM "Re&voke...",                  IDM_REVOKE, GRAYED
    MENUITEM "Imp&ort...",		    IDM_POPULATE, GRAYED
    MENUITEM "&Export Data...",             IDM_EXPORT, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "&Find...\tCtrl+F",            IDM_FIND, GRAYED
    MENUITEM "&Locate...\tCtrl+L",          IDM_LOCATE, GRAYED
    MENUITEM SEPARATOR
    MENUITEM "Reg&ister as Link...",        IDM_REGISTERASLINK, GRAYED
    MENUITEM "Refresh Lin&k...",            IDM_REFRESHLINK, GRAYED
    MENUITEM "Remove...",                   IDM_REMOVEOBJECT, GRAYED
  END

  POPUP "&View"
  BEGIN
    MENUITEM "E&xpand One Level\t+",            IDM_CLASSB_EXPANDONE     , GRAYED
    MENUITEM "Expand &Branch\t*",               IDM_CLASSB_EXPANDBRANCH  , GRAYED
    MENUITEM "Expand &All Branches\tCtrl+*",    IDM_CLASSB_EXPANDALL     , GRAYED
